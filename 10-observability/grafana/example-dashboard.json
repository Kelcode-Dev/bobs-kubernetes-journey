{
  "dashboard": {
    "id": null,
    "uid": "example-dashboard",
    "title": "Application Observability",
    "timezone": "browser",
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "node_cpu_seconds_total",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes",
            "legendFormat": "Memory Available %",
            "refId": "B"
          }
        ]
      }
    ]
  }
}
